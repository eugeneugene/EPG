<Window x:Class="BFM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BFM"
        xmlns:localcommands="clr-namespace:BFM.Commands"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.CommandBindings>
        <CommandBinding
            Command="localcommands:BFMCommands.BloomFilterBrowseCommand"
            Executed="BloomFilterBrowseCommandExecuted" />
    </Window.CommandBindings>
    <StackPanel>
        <GroupBox Header="Bloom Filter" Height="150">
            <Button Content="Open" Width="80" Height="25"  Style="{StaticResource PressButtonEffectStyle}"         
                    Command="localcommands:BFMCommands.BloomFilterBrowseCommand"
                    />

        </GroupBox>
    </StackPanel>
</Window>
