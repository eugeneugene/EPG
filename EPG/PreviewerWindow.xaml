<Window
    x:Class="EPG.PreviewerWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:EPG.Printing.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:localconfig="clr-namespace:EPG.Configuration;assembly=Configuration"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Print preview"
    Width="800"
    Height="450"
    d:DataContext="{d:DesignInstance Type=controls:PrintPreviewerResultPage}"
    localconfig:WindowSettings.Settings="PrintPreviewWindowSettings"
    mc:Ignorable="d">
    <controls:PrintPreviewerControl />
</Window>
